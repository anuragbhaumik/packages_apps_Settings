<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Defines the location of the fingerprint sensor on the device
         0 = back
         1 = front
         2 = left side
         3 = right side
    -->
    <integer name="config_fingerprintSensorLocation">0</integer>

    <!-- Device specific doze package -->
    <string name="config_customDozePackage" translatable="false"></string>

    <!-- Whether to show min/max refresh rate in display settings -->
    <bool name="config_show_refresh_rate_controls">false</bool>

    <!-- Smart Charging -->
    <bool name="config_supportSmartCharging">false</bool>

    <!-- Battery Health -->
    <string name="config_batCurCap"></string>
    <string name="config_batDesCap"></string>
    <string name="config_batChargeCycle"></string>
    <bool name="config_supportBatteryHealth">true</bool>
 
    <!-- Whether to show or hide sensitive info (like IMEI and Phone Number) on About phone -->
    <bool name="configShowDeviceSensitiveInfo">false</bool>

    <!-- Whether assist gesture settings should be enabled -->
    <bool name="config_should_enable_assist_gesture_settings">true</bool>

</resources>
